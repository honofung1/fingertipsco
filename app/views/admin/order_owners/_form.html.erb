<div class="row">
  <div class="col-md-6">
    <div class="card">
      <div class="card-body">
        <%= f.input :name %>
        <%= f.input :order_code_prefix %>
        <%= f.input :telephone %>
        <%= f.input :addresses %>
        <%= f.input :handling_fee %>
      </div>

      <div class="card-footer">
        <div class="actions">
          <%= f.submit class: "btn btn-md btn-success" %>
        </div>
      </div>
      
    </div>
  </div>
</div>